{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "spa root": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "https://odbriana7da.blob.core.windows.net/odbrian-spa/index.html"
        },
        "spa root 400": {
            "matchCondition": {
                "route": "{*path}"
            },
            "backendUri": "https://odbriana7da.blob.core.windows.net/odbrian-spa/index.html"
        },
        "spa root file": {
            "matchCondition": {
                "route": "/{file}.{ending}"
            },
            "backendUri": "https://odbriana7da.blob.core.windows.net/odbrian-spa/{file}.{ending}"
        }
    }
}
