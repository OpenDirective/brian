{
    "rules": {
        "member-access": true,
        "no-reference": true,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration",
            "member-variable-declaration"
        ],
        "curly": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-null-keyword": true,
        "no-shadowed-variable": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "switch-default": true,
        "triple-equals": true,
        "use-isnan": true,
        "prefer-const": true,
        "class-name": true,
        "no-angle-bracket-type-assertion": true,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ]
    } //,
    //    "extends": [
    //    "tslint:latest",
    //        "tslint-config-prettier"
    //    ]
}
